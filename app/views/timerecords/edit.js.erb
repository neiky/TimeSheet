$("#modal-dialog").dialog({
  autoOpen: true,
  height: 450,
  width: 500,
  modal: true,
  title: 'Edit Timerecord',
  buttons: {
    Cancel: function() {
		$( this ).dialog( "close" );
    }
  },
  open: function() {
    $("#modal-dialog").html("<%= escape_javascript(render('form')) %>");
    $("#modal-dialog").find('#dateSelect').datepicker({dateFormat: "yy-mm-dd", showWeek: true, firstDay: 1});
  },
});